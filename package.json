{
	"name": "cambo-geo-index",
	"version": "1.0.0",
	"description": "Cambodia Gazetteer - Geographical Index Open API",
	"private": true,
	"type": "module",
	"engines": {
		"node": ">=20.0.0"
	},
	"scripts": {
		"dev:app": "vite --port 5173",
		"dev:api": "wrangler dev --port 8787",
		"dev": "concurrently pnpm:dev:api pnpm:dev:app",
		"build": "tsc -b && vite build",
		"cf-typegen": "wrangler types",
		"check": "tsc && vite build && wrangler deploy --dry-run",
		"deploy": "wrangler deploy",
		"lint": "eslint .",
		"preview": "npm run build && vite preview"
	},
	"dependencies": {
		"hono": "4.8.2",
		"react": "19.0.0",
		"react-dom": "19.0.0"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "1.14.0",
		"@eslint/js": "9.29.0",
		"@types/node": "24.0.4",
		"@types/react": "19.0.10",
		"@types/react-dom": "19.0.4",
		"@vitejs/plugin-react": "5.0.0",
		"concurrently": "^9.2.1",
		"eslint": "9.27.0",
		"eslint-plugin-react-hooks": "5.2.0",
		"eslint-plugin-react-refresh": "0.4.20",
		"globals": "16.2.0",
		"typescript": "5.8.3",
		"typescript-eslint": "8.35.0",
		"vite": "^6.4.1",
		"wrangler": "4.46.0"
	},
	"keywords": [
		"cambodia",
		"khmer",
		"geo-data",
		"geodata",
		"gazetteer",
		"api",
		"rest-api",
		"geographical",
		"geography",
		"southeast-asia",
		"asean",
		"hono",
		"typescript",
		"open-data",
		"open-source"
	],
	"author": "Maneth PAK <manethpak.dev@gmail.com>",
	"license": "MIT",
	"homepage": "https://github.com/manethpak/cambo-geo-index#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/manethpak/cambo-geo-index.git"
	},
	"bugs": {
		"url": "https://github.com/manethpak/cambo-geo-index/issues"
	},
	"files": [
		"README.md",
		"LICENSE"
	]
}